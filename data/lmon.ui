<interface>
	<object id="root" class="GtkButton">
		<property name="margin-start">5</property>
		<property name="margin-end">5</property>
	</object>
	<object id="percents_box" class="GtkBox">
		<property name="spacing">5</property>
		<child>
			<object class="GtkLabel">
				<property name="label">CPU</property>
				<property name="valign">baseline</property>
				<style> <class name="small_label" /> </style>
			</object>
		</child>
		<child>
			<object id="cpuvalue" class="GtkLabel">
				<property name="valign">baseline</property>
				<style> <class name="default" /> </style>
			</object>
		</child>
		<child>
			<object class="GtkLabel">
				<property name="label">MEM</property>
				<property name="valign">baseline</property>
				<style> <class name="small_label" /> </style>
			</object>
		</child>
		<child>
			<object id="memvalue" class="GtkLabel">
				<property name="valign">baseline</property>
				<style> <class name="default" /> </style>
			</object>
		</child>
	</object>
	<object id="loads_box" class="GtkBox">
		<property name="spacing">5</property>
		<child>
			<object class="GtkLabel">
				<property name="label">LOAD</property>
				<property name="valign">baseline</property>
				<style> <class name="small_label" /> </style>
			</object>
		</child>
		<child>
			<object id="loadvalue" class="GtkLabel">
				<property name="valign">baseline</property>
				<style> <class name="default" /> </style>
			</object>
		</child>
	</object>
	<object id="aboutwin" class="GtkWindow">
		<property name="title">About xfce4-lmon-plugin</property>
		<property name="resizable">false</property>
		<child>
			<object class="GtkGrid">
				<property name="margin-start">8</property>
				<property name="margin-end">8</property>
				<property name="margin-top">8</property>
				<property name="margin-bottom">8</property>
				<property name="row-spacing">8</property>
				<property name="column-spacing">8</property>
				<child>
					<object class="GtkLabel">
						<property name="label">xfce4-lmon-plugin</property>
						<attributes>
							<attribute name="font-desc" value="Sans 12"/>
						</attributes>
					</object>
					<packing>
						<property name="left-attach">0</property>
						<property name="top-attach">0</property>
						<property name="width">3</property>
					</packing>
				</child>
				<child>
					<object class="GtkImage">
						<property name="margin-start">4</property>
						<property name="margin-end">4</property>
						<property name="icon-name">view-hidden</property>
						<property name="icon_size">6</property>
					</object>
					<packing>
						<property name="left-attach">0</property>
						<property name="top-attach">1</property>
						<property name="height">2</property>
					</packing>
				</child>
				<child>
					<object class="GtkLabel">
						<property name="sensitive">False</property>
						<property name="halign">end</property>
						<property name="valign">end</property>
						<property name="label">Author</property>
					</object>
					<packing>
						<property name="left-attach">1</property>
						<property name="top-attach">1</property>
					</packing>
				</child>
				<child>
					<object class="GtkLabel">
						<property name="halign">start</property>
						<property name="valign">end</property>
						<property name="label">kqq</property>
					</object>
					<packing>
						<property name="left-attach">2</property>
						<property name="top-attach">1</property>
					</packing>
				</child>
				<child>
					<object class="GtkLabel">
						<property name="sensitive">False</property>
						<property name="halign">end</property>
						<property name="valign">start</property>
						<property name="label">Source code</property>
					</object>
					<packing>
						<property name="left-attach">1</property>
						<property name="top-attach">2</property>
					</packing>
				</child>
				<child>
					<object class="GtkLabel">
						<property name="halign">start</property>
						<property name="valign">start</property>
						<property name="label">&lt;a href="https://github.com/kqq9/xfce4-lmon-plugin"&gt;https://github.com/kqq9/xfce4-lmon-plugin&lt;/a&gt;</property>
						<property name="use-markup">True</property>
					</object>
					<packing>
						<property name="left-attach">2</property>
						<property name="top-attach">2</property>
					</packing>
				</child>
				<child>
					<object class="GtkLabel">
						<property name="label" translatable="yes">Copyright Â© kqq 2020-2021</property>
						<attributes>
							<attribute name="font-desc" value="Sans 8"/>
						</attributes>
					</object>
					<packing>
						<property name="left-attach">0</property>
						<property name="top-attach">3</property>
						<property name="width">3</property>
					</packing>
				</child>
			</object>
		</child>
	</object>
</interface>
